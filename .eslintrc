{
"parserOptions": {
        "ecmaVersion": 13
    }
,

"rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "react/jsx-uses-react": "off",
        "react/jsx-uses-vars": "disabled"
    }
,
"extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}
